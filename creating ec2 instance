terraform {
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 4.16"
    }
  }
}

# Configure the AWS Provider
provider "aws" {
  region = "ap-southeast-2"
}

resource "aws_instance" "Appserver" {
    ami           = "ami-056729a8869a8ab24"  # Specify an appropriate AMI ID
    instance_type = "t2.micro"
  }

